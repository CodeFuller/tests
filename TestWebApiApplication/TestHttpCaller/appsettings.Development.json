{
	"testAppUri": "http://localhost:51099/test",

	"serilog": {
		"using": [
			"Serilog.Sinks.Console"
		],

		"writeTo": [
			{
				"name": "Console",
				"args": {
					"restrictedToMinimumLevel": "Information",
					"outputTemplate": "{Timestamp:yyyy.MM.dd HH:mm:ss.fff} [{Level:u3}] {Message}{NewLine}{Exception}"
				}
			}
		]
	}
}
